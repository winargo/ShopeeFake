<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <android.support.constraint.ConstraintLayout
        android:id="@+id/timeline1"
        android:layout_width="0dp"
        android:layout_height="329dp"
        android:background="#ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <View
            android:id="@+id/garis_atas_A"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#D3D3D3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/nama_toko_A"
            android:layout_width="wrap_content"
            android:layout_height="26dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="24dp"
            android:text="namatoko"
            android:textColor="@android:color/darker_gray"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/profile_penjual_A"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button_ikuti_A"
            style="@style/Widget.AppCompat.Button"
            android:layout_width="61dp"
            android:layout_height="21dp"
            android:layout_marginRight="16dp"
            android:layout_marginStart="172dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/button_bg_stroke"
            android:text="+ ikuti"
            android:textAllCaps="false"
            android:textColor="@color/ColorPrimary"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/nama_toko_A"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/profile_penjual_A"
            android:layout_width="34dp"
            android:layout_height="34dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/toko_circle"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/gambar_barang_A1"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_marginStart="6dp"
            android:layout_marginTop="56dp"
            android:src="@drawable/temp_barang"
            app:layout_constraintEnd_toStartOf="@+id/gambar_barang_A2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/gambar_barang_A2"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_marginTop="56dp"
            android:src="@drawable/temp_barang"
            app:layout_constraintEnd_toStartOf="@+id/gambar_barang_A3"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/gambar_barang_A1"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/gambar_barang_A3"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_marginEnd="6dp"
            android:layout_marginTop="56dp"
            android:src="@drawable/temp_barang"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/gambar_barang_A2"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/gambar_barang_A4"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_marginStart="6dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/temp_barang"
            app:layout_constraintEnd_toStartOf="@+id/gambar_barang_A5"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gambar_barang_A1" />

        <ImageButton
            android:id="@+id/gambar_barang_A5"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/temp_barang"
            app:layout_constraintEnd_toStartOf="@+id/gambar_barang_A6"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/gambar_barang_A4"
            app:layout_constraintTop_toBottomOf="@+id/gambar_barang_A2" />

        <ImageButton
            android:id="@+id/gambar_barang_A6"
            android:layout_width="110dp"
            android:layout_height="100dp"
            android:layout_marginEnd="6dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/temp_barang"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/gambar_barang_A5"
            app:layout_constraintTop_toBottomOf="@+id/gambar_barang_A3" />

        <TextView
            android:id="@+id/deskripsiA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="Deskripsi barang"
            android:textColor="@android:color/black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/gambar_barang_A4" />

        <View
            android:id="@+id/garis_tengah_A1"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginTop="300dp"
            android:background="@drawable/gradient"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/nomor_love_A1"
            android:layout_width="115dp"
            android:layout_height="22dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:background="#FFFFFF"
            android:clickable="true"
            android:focusable="true"
            android:text="0"
            android:textAlignment="center"
            android:textColor="#808080"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="@+id/gambar_love_A"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintHorizontal_chainStyle="spread_inside"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/garis_tengah_A1" />

        <View
            android:id="@+id/garis_vertical_A1"
            android:layout_width="1dp"
            android:layout_height="20dp"
            android:layout_marginTop="4dp"
            android:background="#DEDEDE"
            app:layout_constraintEnd_toStartOf="@+id/komentar_A1"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/nomor_love_A1"
            app:layout_constraintTop_toBottomOf="@+id/garis_tengah_A1" />


        <TextView
            android:id="@+id/komentar_A1"
            android:layout_width="115dp"
            android:layout_height="22dp"
            android:layout_marginTop="4dp"
            android:background="#FFFFFF"
            android:clickable="true"
            android:focusable="true"
            android:text="Komentar"
            android:textAlignment="center"
            android:textColor="#808080"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@+id/garis_vertical_A2"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/garis_vertical_A1"
            app:layout_constraintTop_toBottomOf="@+id/garis_tengah_A1" />

        <View
            android:id="@+id/garis_vertical_A2"
            android:layout_width="1dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="4dp"
            android:background="#DEDEDE"
            app:layout_constraintEnd_toStartOf="@+id/share_A"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/komentar_A1"
            app:layout_constraintTop_toBottomOf="@+id/garis_tengah_A1" />

        <TextView
            android:id="@+id/share_A"
            android:layout_width="115dp"
            android:layout_height="22dp"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="16dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:background="#FFFFFF"
            android:clickable="true"
            android:focusable="true"
            android:text="Share"
            android:textAlignment="center"
            android:textColor="#808080"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintStart_toEndOf="@+id/garis_vertical_A2"
            app:layout_constraintTop_toBottomOf="@+id/garis_tengah_A1" />

        <ImageView
            android:id="@+id/gambar_share_A"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:src="@android:drawable/ic_menu_share"
            android:tint="#808080"
            app:layout_constraintEnd_toEndOf="@+id/share_A"
            app:layout_constraintHorizontal_bias="0.177"
            app:layout_constraintStart_toEndOf="@+id/garis_vertical_A2"
            app:layout_constraintTop_toTopOf="@+id/komentar_A1"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/gambar_koment_A"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:src="@drawable/ic_comment"
            app:layout_constraintEnd_toStartOf="@+id/garis_vertical_A2"
            app:layout_constraintHorizontal_bias="0.023"
            app:layout_constraintStart_toEndOf="@+id/garis_vertical_A1"
            app:layout_constraintTop_toTopOf="@+id/komentar_A1" />

        <ImageView
            android:id="@+id/gambar_love_A"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="36dp"
            android:src="@drawable/ic_love"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/komentar_A1" />

        <View
            android:id="@+id/garis_bawah_A"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#D3D3D3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent" />

    </android.support.constraint.ConstraintLayout>
</android.support.constraint.ConstraintLayout>